{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es2017",
    "skipLibCheck": true,
    "strict": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}